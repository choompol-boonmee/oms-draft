         </xs:element>
         <xs:element name="WorkTask"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:WorkTask"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#TroubleOrder.WorkTask">
            <xs:annotation>
               <xs:documentation>The work tasks that are associated with this trouble order.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="TroubleReportingKind"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#TroubleReportingKind">
      <xs:annotation>
         <xs:documentation>Kind of trouble reporting.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="app">
            <xs:annotation/>
         </xs:enumeration>
         <xs:enumeration value="call">
