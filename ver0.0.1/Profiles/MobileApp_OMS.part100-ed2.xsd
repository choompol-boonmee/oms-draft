                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.completedDateTime">
            <xs:annotation>
               <xs:documentation>Date and time work task was completed.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="contractorCost"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Money"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.contractorCost">
            <xs:annotation>
               <xs:documentation>Total contractor costs associated with the work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="crewETA"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.crewETA">
            <xs:annotation>
               <xs:documentation>Estimated time of arrival, so that customer or police/fire department can be informed when the crew will arrive.</xs:documentation>
            </xs:annotation>
