                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.estimatedCompletionTime">
            <xs:annotation>
               <xs:documentation>Time and Date when the work task will be completed.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="instruction"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.instruction">
            <xs:annotation>
               <xs:documentation>Instructions for performing this task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="kind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ServiceRequestKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#BaseWork.kind">
            <xs:annotation>
               <xs:documentation>Kind of work.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="laborCost"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Money"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.laborCost">
            <xs:annotation>
               <xs:documentation>Total labor costs associated with the work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="laborHours"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Hours"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.laborHours">
            <xs:annotation>
               <xs:documentation>Hours of labor expended under work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="materiallCost"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Money"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.materiallCost">
            <xs:annotation>
               <xs:documentation>Total material costs associated with the work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="schedOverride"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.schedOverride">
            <xs:annotation>
               <xs:documentation>If specified, override schedule and perform this task in accordance with instructions specified here.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="startedDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.startedDateTime">
            <xs:annotation>
               <xs:documentation>Date and time work task was started.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="statusKind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:WorkStatusKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#BaseWork.statusKind">
            <xs:annotation>
               <xs:documentation>Kind of work status.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="taskKind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:WorkTaskKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.taskKind">
            <xs:annotation>
               <xs:documentation>Kind of work.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="toolCost"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Money"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.toolCost">
            <xs:annotation>
               <xs:documentation>Total tool costs associated with the work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="WorkActivityRecords"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:WorkActivityRecord"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#BaseWork.WorkActivityRecords">
            <xs:annotation>
               <xs:documentation>All activity records for this work or work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="WorkLocation"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:WorkLocation"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#BaseWork.WorkLocation">
            <xs:annotation>
               <xs:documentation>Location for this work/task.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
