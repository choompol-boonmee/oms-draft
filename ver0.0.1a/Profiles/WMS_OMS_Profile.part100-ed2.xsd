               <xs:documentation>Switching action to perform.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="phases"
                     minOccurs="1"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#SwitchingAction.phases">
