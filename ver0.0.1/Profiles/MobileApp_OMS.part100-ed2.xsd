               <xs:documentation>Trouble reported through interactive voice response system.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="letter">
            <xs:annotation>
               <xs:documentation>Trouble reported by letter.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
