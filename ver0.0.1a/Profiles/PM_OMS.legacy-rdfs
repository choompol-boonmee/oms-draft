  <rdf:Description rdf:about="#Status.remark">
    <cims:dataType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment>Pertinent information regarding the current 'value', as free form text.</rdfs:comment>
    <rdfs:domain rdf:resource="#Status"/>
    <rdfs:label>remark</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#Status.value">
    <cims:dataType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment>Status value at 'dateTime'; prior status changes may have been kept in instances of activity records associated with the object to which this status applies.</rdfs:comment>
    <rdfs:domain rdf:resource="#Status"/>
    <rdfs:label>value</rdfs:label>
  </rdf:Description>
