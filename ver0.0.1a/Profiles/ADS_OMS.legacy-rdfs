  <rdf:Description rdf:about="#Asset.electronicAddress">
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment>Electronic address.</rdfs:comment>
    <rdfs:domain rdf:resource="#Asset"/>
    <rdfs:label>electronicAddress</rdfs:label>
    <rdfs:range rdf:resource="#ElectronicAddress"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Asset.inUseDate">
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment>In use dates for this asset.</rdfs:comment>
    <rdfs:domain rdf:resource="#Asset"/>
    <rdfs:label>inUseDate</rdfs:label>
    <rdfs:range rdf:resource="#InUseDate"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Asset.inUseState">
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
