   <xs:complexType name="OutageArea"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#OutageArea">
      <xs:annotation>
         <xs:documentation>This defines the area covered by the Outage.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="earliestReportedTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#OutageArea.earliestReportedTime">
