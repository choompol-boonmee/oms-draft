      <xs:sequence>
         <xs:element name="end"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#DateTimeInterval.end">
            <xs:annotation>
               <xs:documentation>End date and time of this interval.  The end date and time where the interval is defined up to, but excluded.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="start"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#DateTimeInterval.start">
            <xs:annotation>
               <xs:documentation>Start date and time of this interval.  The start date and time is included in the defined interval.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ElectronicAddress"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#ElectronicAddress">
      <xs:annotation>
         <xs:documentation>Electronic address information.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="email1"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ElectronicAddress.email1">
            <xs:annotation>
               <xs:documentation>Primary email address.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Equipment"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#Equipment">
      <xs:annotation>
         <xs:documentation>The parts of a power system that are physical devices, electronic or mechanical.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="inService"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:boolean"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Equipment.inService">
            <xs:annotation>
               <xs:documentation>Specifies the availability of the equipment. True means the equipment is available for topology processing, which determines if the equipment is energized or not. False means that the equipment is treated by network applications as if it is not in the model.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="normallyInService"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:boolean"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Equipment.normallyInService">
            <xs:annotation>
               <xs:documentation>Specifies the availability of the equipment under normal operating conditions. True means the equipment is available for topology processing, which determines if the equipment is energized or not. False means that the equipment is treated by network applications as if it is not in the model.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Names"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:Name"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.Names">
            <xs:annotation>
               <xs:documentation>All names of this identified object.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Fault" sawsdl:modelReference="http://iec.ch/TC57/CIM100#Fault">
      <xs:annotation>
         <xs:documentation>Abnormal condition causing current flow through conducting equipment, such as caused by equipment failure or short circuits from objects not typically modelled (for example, a tree falling on a line).</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="kind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PhaseConnectedFaultKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Fault.kind">
            <xs:annotation>
               <xs:documentation>The kind of phase fault.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="occurredDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Fault.occurredDateTime">
            <xs:annotation>
               <xs:documentation>The date and time at which the fault occurred.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="phases"
                     minOccurs="1"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Fault.phases">
            <xs:annotation>
               <xs:documentation>The phases participating in the fault. The fault connections into these phases are further specified by the type of fault.</xs:documentation>
            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="FaultCauseTypes"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:FaultCauseType"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Fault.FaultCauseTypes">
            <xs:annotation>
               <xs:documentation>All types of fault cause.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FaultyEquipment"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Equipment"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Fault.FaultyEquipment">
            <xs:annotation>
               <xs:documentation>Equipment carrying this fault.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="FaultCauseType"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#FaultCauseType">
      <xs:annotation>
         <xs:documentation>Type of cause of the fault.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="description"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.description">
            <xs:annotation>
               <xs:documentation>The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="FieldDispatchHistory"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#FieldDispatchHistory">
      <xs:annotation>
         <xs:documentation>The history of field dispatch statuses for this work.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Crew"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Crew"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#FieldDispatchHistory.Crew">
            <xs:annotation>
               <xs:documentation>The crew that has this field dispatch history.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PlannedOutage"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PlannedOutage"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#FieldDispatchHistory.PlannedOutage">
            <xs:annotation>
               <xs:documentation>The planned outage that has the field dispatch history.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="UnplannedOutage"
                     minOccurs="1"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#FieldDispatchHistory.UnplannedOutage">
            <xs:annotation>
               <xs:documentation>The unplanned outage that has the field dispatch history.</xs:documentation>
            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Incident"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#Incident">
      <xs:annotation>
         <xs:documentation>Description of a problem in the field that may be reported in a trouble ticket or come from another source. It may have to do with an outage.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="cause"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Incident.cause">
            <xs:annotation>
               <xs:documentation>Cause of this incident.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="comment"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.comment">
            <xs:annotation>
               <xs:documentation>Free text comment.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="createdDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.createdDateTime">
            <xs:annotation>
               <xs:documentation>Date and time that this document was created.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="subject"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.subject">
            <xs:annotation>
               <xs:documentation>Document subject.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="title"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.title">
            <xs:annotation>
               <xs:documentation>Document title.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Name" sawsdl:modelReference="http://iec.ch/TC57/CIM100#Name">
      <xs:annotation>
         <xs:documentation>The Name class provides the means to define any number of human readable  names for an object. A name is &lt;b&gt;not&lt;/b&gt; to be used for defining inter-object relationships. For inter-object relationships instead use the object identification 'mRID'.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="name"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Name.name">
            <xs:annotation>
               <xs:documentation>Any free text that name the object.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IdentifiedObject"
                     minOccurs="1"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Name.IdentifiedObject">
            <xs:annotation>
               <xs:documentation>Identified object that this name designates.</xs:documentation>
            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="NameType"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:NameType"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Name.NameType">
            <xs:annotation>
               <xs:documentation>Type of this name.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="NameType"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#NameType">
      <xs:annotation>
         <xs:documentation>Type of name. Possible values for attribute 'name' are implementation dependent but standard profiles may specify types. An enterprise may have multiple IT systems each having its own local name for the same object, e.g. a planning system may have different names from an EMS. An object may also have different names within the same IT system, e.g. localName as defined in CIM version 14. The definition from CIM14 is:</xs:documentation>
         <xs:documentation>The localName is a human readable name of the object. It is a free text name local to a node in a naming hierarchy similar to a file directory structure. A power system related naming hierarchy may be: Substation, VoltageLevel, Equipment etc. Children of the same parent in such a hierarchy have names that typically are unique among them.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
   </xs:complexType>
   <xs:complexType name="Outage" sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage">
      <xs:annotation>
         <xs:documentation>Document describing details of an active or planned outage in a part of the electrical network.</xs:documentation>
         <xs:documentation>A non-planned outage may be created upon:</xs:documentation>
         <xs:documentation>- a breaker trip,</xs:documentation>
         <xs:documentation>- a fault indicator status change,</xs:documentation>
         <xs:documentation>- a meter event indicating customer outage,</xs:documentation>
         <xs:documentation>- a reception of one or more customer trouble calls, or</xs:documentation>
         <xs:documentation>- an operator command, reflecting information obtained from the field crew.</xs:documentation>
         <xs:documentation>Outage restoration may be performed using a switching plan which complements the outage information with detailed switching activities, including the relationship to the crew and work.</xs:documentation>
         <xs:documentation>A planned outage may be created upon:</xs:documentation>
         <xs:documentation>- a request for service, maintenance or construction work in the field, or</xs:documentation>
         <xs:documentation>- an operator-defined outage for what-if/contingency network analysis.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="cause"
                     minOccurs="0"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#UnplannedOutage.cause">
            <xs:annotation>
               <xs:documentation>The cause of this outage.  This is the cause that is used to present to external entities.  That is, the cause is weather, equipment failure, etc.</xs:documentation>
               <xs:documentation>Note: At present, this is a free string text; it could be replaced with an enumeration in the future.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="causeKind"
                     minOccurs="0"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#UnplannedOutage.causeKind">
            <xs:annotation>
               <xs:documentation>The possible cause that could be associated with this unplanned outage.</xs:documentation>
            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="createdDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.createdDateTime">
            <xs:annotation>
               <xs:documentation>Date and time that this document was created.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="customersRestored"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:integer"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.customersRestored">
            <xs:annotation>
               <xs:documentation>number of customers that have been restored in the area.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="lastModifiedDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.lastModifiedDateTime">
            <xs:annotation>
               <xs:documentation>Date and time this document was last modified. Documents may potentially be modified many times during their lifetime.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="metersAffected"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:integer"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.metersAffected">
            <xs:annotation>
               <xs:documentation>The updated number of meters affected by the outage as reported by the OMS within the utility.  It is assumed this number will be updated repeatedly until the full outage is resolved.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="originalCustomersServed"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:integer"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.originalCustomersServed">
            <xs:annotation>
               <xs:documentation>the total number of customers that are served in the area (both outaged and not outaged).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="originalMetersAffected"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:integer"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.originalMetersAffected">
            <xs:annotation>
               <xs:documentation>The original number of meters that were affected as reported by the OMS within the utility. That is, this is the total number of meters that were out at the beginning of the outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="outageKind"
                     minOccurs="1"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.outageKind">
            <xs:annotation>
               <xs:documentation>Defines if the outage has been verified or is only estimated</xs:documentation>
            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="reason"
                     minOccurs="0"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PlannedOutage.reason">
            <xs:annotation>
               <xs:documentation>The reason for the planned outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="reportedStartTime"
                     minOccurs="0"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#UnplannedOutage.reportedStartTime">
            <xs:annotation>
               <xs:documentation>The earliest start time of the Outage - as reported by some system or individual</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="statusKind"
                     minOccurs="1"
                     maxOccurs="1"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.statusKind">
            <xs:annotation>
               <xs:documentation>defines the status of the crew as in dispatched or arrived, etc.</xs:documentation>
            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="subject"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.subject">
            <xs:annotation>
               <xs:documentation>Document subject.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="title"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.title">
            <xs:annotation>
               <xs:documentation>Document title.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="type"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.type">
            <xs:annotation>
               <xs:documentation>Utility-specific classification of this document, according to its corporate standards, practices, and existing IT systems (e.g., for management of assets, maintenance, work, outage, customers, etc.).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ActivityRecord"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:ActivityRecord"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.ActivityRecord">
            <xs:annotation>
               <xs:documentation>The activity record for a document</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="actualPeriod"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:DateTimeInterval"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.actualPeriod">
            <xs:annotation>
               <xs:documentation>Actual outage period; end of the period corresponds to the actual restoration time.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DeEnergizedUsagePoint"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:UsagePoint"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.DeEnergizedUsagePoint">
            <xs:annotation>
               <xs:documentation>all deenergized useage points associated with the outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="docStatus"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Status"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.docStatus">
            <xs:annotation>
               <xs:documentation>Status of this document. For status of subject matter this document represents (e.g., Agreement, Work), use 'status' attribute.</xs:documentation>
               <xs:documentation>Example values for 'docStatus.status' are draft, approved, cancelled, etc.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="electronicAddress"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ElectronicAddress"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.electronicAddress">
            <xs:annotation>
               <xs:documentation>Electronic address.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="EnergizedUsagePoint"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:UsagePoint"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.EnergizedUsagePoint">
            <xs:annotation>
               <xs:documentation>All energized usage points associated with this outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Equipments"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:Equipment"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.Equipments">
            <xs:annotation>
               <xs:documentation>All equipments associated with this outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="estimatedPeriod"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:DateTimeInterval"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.estimatedPeriod">
            <xs:annotation>
               <xs:documentation>Estimated outage period for a planned outage. The start of the period is the start of the planned outage and the end of the period corresponds to the end of the planned outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Faults"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:Fault"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.Faults">
            <xs:annotation>
               <xs:documentation>All faults involved in this outage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FieldDispatchHistory"
                     minOccurs="0"
                     maxOccurs="1"
                     type="m:FieldDispatchHistory"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#UnplannedOutage.FieldDispatchHistory">
            <xs:annotation>
               <xs:documentation>The dispatch history associated with the unplanned outage</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FieldDispatchHistory"
                     minOccurs="0"
                     maxOccurs="1"
                     type="m:FieldDispatchHistory"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PlannedOutage.FieldDispatchHistory">
            <xs:annotation>
