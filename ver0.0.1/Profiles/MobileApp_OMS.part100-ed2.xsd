            </xs:annotation>
         </xs:element>
         <xs:element name="CrewMembers"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:CrewMember"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Crew.CrewMembers">
            <xs:annotation>
               <xs:documentation>All members of this crew.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CrewType"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:CrewType"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Crew.CrewType">
            <xs:annotation>
               <xs:documentation>Type of this crew.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="status"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Status"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Crew.status">
            <xs:annotation>
               <xs:documentation>Status of this crew.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SwitchingAction"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:SwitchingAction"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Crew.SwitchingAction">
            <xs:annotation>
               <xs:documentation>The switching action that is assigned to this crew.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="WorkTasks"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:WorkTask"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Crew.WorkTasks">
            <xs:annotation>
