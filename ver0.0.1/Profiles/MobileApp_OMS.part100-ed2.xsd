   <xs:annotation/>
   <xs:import namespace="http://iec.ch/TC57/2021/schema/message"
              schemaLocation="AbstractPayloadType.xsd"/>
   <xs:element name="MobileApp_OMS_Profile"
               type="m:MobileApp_OMS_Profile"
