    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment>Condition of asset at last baseline. Examples include new, rebuilt, overhaul required, other. Refer to inspection data for information on the most current condition of the asset.</rdfs:comment>
    <rdfs:domain rdf:resource="#Asset"/>
    <rdfs:label>initialCondition</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#Asset.initialLossOfLife">
    <cims:dataType rdf:resource="#PerCent"/>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
    <cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment>Percentage of initial life expectancy that has been lost as of the last life expectancy baseline. Represents &#xD;
(initial life expectancy - current life expectancy) / initial life expectancy.</rdfs:comment>
    <rdfs:domain rdf:resource="#Asset"/>
    <rdfs:label>initialLossOfLife</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#Asset.kind">
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
