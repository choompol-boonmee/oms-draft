                     type="m:SafetyDocument"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#FieldSafetySupervisor.ReleasedSafetyDocuments">
            <xs:annotation>
               <xs:documentation>All safety documents released by this supervisor.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="CrewStatusKind"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#CrewStatusKind">
      <xs:annotation>
         <xs:documentation>Defines the current status of the Crew - assigned, arrived, etc.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="arrived">
            <xs:annotation>
               <xs:documentation>the crew is on site at the outage location</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="assigned">
            <xs:annotation>
               <xs:documentation>the crew has been assigned to work on a task</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="awaitingCrewAssignment">
            <xs:annotation>
               <xs:documentation>Indicates that the work is awaiting one or more crews to be assigned</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="enroute">
            <xs:annotation>
               <xs:documentation>the crew has been dispatched to work on an outage</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="fieldComplete">
            <xs:annotation>
               <xs:documentation>the crew has corrected the outage in the field.  The equipment or other devices may not be energized at this time.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
