               <xs:documentation>All crews participating in this work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ProcedureDataSet"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:ProcedureDataSet"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#WorkTask.ProcedureDataSet">
            <xs:annotation>
               <xs:documentation>Procedure data set associated with this work task.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="PerCent"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#PerCent">
