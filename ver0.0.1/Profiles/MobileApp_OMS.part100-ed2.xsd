               <xs:element name="summary"
                           minOccurs="1"
                           maxOccurs="1"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#Outage.summary">
                  <xs:annotation>
                     <xs:documentation>Summary counts of service points (customers) affected by this outage.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType sawsdl:modelReference="">
                     <xs:attribute name="ref" type="xs:string" use="required"/>
                     <xs:attribute name="referenceType" type="xs:string" use="optional"/>
                  </xs:complexType>
               </xs:element>
               <xs:element name="TroubleOrder"
                           minOccurs="1"
                           maxOccurs="unbounded"
                           sawsdl:modelReference="http://iec.ch/TC57/CIM100#UnplannedOutage.TroubleOrder">
