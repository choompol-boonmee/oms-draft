               <xs:documentation>X axis position.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="yPosition"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#PositionPoint.yPosition">
