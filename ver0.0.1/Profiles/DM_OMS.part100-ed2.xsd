            </xs:annotation>
            <xs:complexType sawsdl:modelReference="">
               <xs:attribute name="ref" type="xs:string" use="required"/>
               <xs:attribute name="referenceType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="RetiredReasonKind"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#RetiredReasonKind">
      <xs:annotation>
         <xs:documentation>Reason asset retired.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="environmental">
            <xs:annotation>
               <xs:documentation>Retired due to environmental reasons.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="excessiveMaintenance">
            <xs:annotation>
               <xs:documentation>Retired due to excessive maintainance issues.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="facilitiesUpgrade">
            <xs:annotation>
               <xs:documentation>Retired due to facility upgrade.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="failed">
            <xs:annotation>
               <xs:documentation>Retired because of failure.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="obsolescence">
            <xs:annotation>
               <xs:documentation>Retired due to obsolescence.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="other">
            <xs:annotation>
               <xs:documentation>Retired due to other reasons.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="sold">
