<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:m="http://pea.co.th/cim/profile/DTMS_OMS#"
           xmlns:part100="http://iec.ch/TC57/2021/schema/message"
           xmlns:sawsdl="http://www.w3.org/ns/sawsdl"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://pea.co.th/cim/profile/DTMS_OMS#"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">
   <xs:annotation/>
   <xs:import namespace="http://iec.ch/TC57/2021/schema/message"
              schemaLocation="AbstractPayloadType.xsd"/>
   <xs:element name="DTMS_OMS_Profile"
               type="m:DTMS_OMS_Profile"
               substitutionGroup="part100:AbstractPayload"/>
   <xs:complexType name="DTMS_OMS_Profile">
      <xs:complexContent>
         <xs:extension base="part100:AbstractPayloadType">
            <xs:sequence>
               <xs:element name="ActivityRecord"
                           type="m:ActivityRecord"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
               <xs:element name="Outage"
                           type="m:Outage"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
               <xs:element name="PowerTransformer"
                           type="m:PowerTransformer"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
               <xs:element name="TransformerTankInfo"
                           type="m:TransformerTankInfo"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ActivityRecord"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#ActivityRecord">
      <xs:annotation>
         <xs:documentation>Records activity for an entity at a point in time; activity may be for an event that has already occurred or for a planned activity.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="createdDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ActivityRecord.createdDateTime">
            <xs:annotation>
               <xs:documentation>Date and time this activity record has been created (different from the 'status.dateTime', which is the time of a status change of the associated object, if applicable).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="description"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.description">
            <xs:annotation>
               <xs:documentation>The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="name"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.name">
            <xs:annotation>
               <xs:documentation>The name is any free human readable and possibly non unique text naming the object.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="reason"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ActivityRecord.reason">
            <xs:annotation>
               <xs:documentation>Reason for event resulting in this activity record, typically supplied when user initiated.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="severity"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ActivityRecord.severity">
            <xs:annotation>
               <xs:documentation>Severity level of event resulting in this activity record.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="type"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#ActivityRecord.type">
            <xs:annotation>
               <xs:documentation>Type of event resulting in this activity record.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Analytic"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#Analytic">
      <xs:annotation>
         <xs:documentation>An algorithm or calculation for making an assessment about an asset or asset grouping for lifecycle decision making.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="bestValue"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Analytic.bestValue">
            <xs:annotation>
               <xs:documentation>Value that indicates best possible numeric value.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="createdDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.createdDateTime">
            <xs:annotation>
               <xs:documentation>Date and time that this document was created.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="description"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.description">
            <xs:annotation>
               <xs:documentation>The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="kind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AnalyticKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Analytic.kind">
            <xs:annotation>
               <xs:documentation>Kind of analytic this analytic is.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="lastModifiedDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Document.lastModifiedDateTime">
            <xs:annotation>
               <xs:documentation>Date and time this document was last modified. Documents may potentially be modified many times during their lifetime.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="name"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.name">
            <xs:annotation>
               <xs:documentation>The name is any free human readable and possibly non unique text naming the object.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="scaleKind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:ScaleKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Analytic.scaleKind">
            <xs:annotation>
               <xs:documentation>The scoring scale kind.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="worstValue"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Analytic.worstValue">
            <xs:annotation>
               <xs:documentation>Value that indicates worst possible numeric value.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="AnalyticKind"
                  sawsdl:modelReference="http://iec.ch/TC57/CIM100#AnalyticKind">
      <xs:annotation>
         <xs:documentation>Possible kinds of analytics.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="agingAnalytic">
            <xs:annotation>
               <xs:documentation>Analytic evaluates aging.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="faultAnalytic">
            <xs:annotation>
               <xs:documentation>Analytic evaluates fault.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="healthAnalytic">
            <xs:annotation>
               <xs:documentation>Analytic evaluates asset health.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="other">
            <xs:annotation>
               <xs:documentation>Analytic evaluates other factor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="replacementAnalytic">
            <xs:annotation>
               <xs:documentation>Analytic evaluates need for replacement.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="riskAnalytic">
            <xs:annotation>
               <xs:documentation>Analytic evaluates risk.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="AnalyticScore"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#AnalyticScore">
      <xs:annotation>
         <xs:documentation>An indicative scoring by an analytic that can be used to characterize the health of or the risk associated with one or more assets.  The analytic score reflects the results of an execution of an analytic against an asset or group of assets.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="calculationDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#AnalyticScore.calculationDateTime">
            <xs:annotation>
               <xs:documentation>Timestamp of when the score was calculated.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="description"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.description">
            <xs:annotation>
               <xs:documentation>The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="effectiveDateTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#AnalyticScore.effectiveDateTime">
            <xs:annotation>
               <xs:documentation>Date-time for when the score applies.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="name"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.name">
            <xs:annotation>
               <xs:documentation>The name is any free human readable and possibly non unique text naming the object.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="value"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#AnalyticScore.value">
            <xs:annotation>
               <xs:documentation>Asset health score value.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Asset" sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset">
      <xs:annotation>
         <xs:documentation>Tangible resource of the utility, including power system equipment, various end devices, cabinets, buildings, etc. For electrical network equipment, the role of the asset is defined through PowerSystemResource and its subclasses, defined mainly in the Wires model (refer to IEC61970-301 and model package IEC61970::Wires). Asset description places emphasis on the physical characteristics of the equipment fulfilling that role.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="aliasName"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.aliasName">
            <xs:annotation>
               <xs:documentation>The aliasName is free text human readable name of the object alternative to IdentifiedObject.name. It may be non unique and may not correlate to a naming hierarchy.</xs:documentation>
               <xs:documentation>The attribute aliasName is retained because of backwards compatibility between CIM relases. It is however recommended to replace aliasName with the Name class as aliasName is planned for retirement at a future time.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="description"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.description">
            <xs:annotation>
               <xs:documentation>The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="initialCondition"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.initialCondition">
            <xs:annotation>
               <xs:documentation>Condition of asset at last baseline. Examples include new, rebuilt, overhaul required, other. Refer to inspection data for information on the most current condition of the asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="initialLossOfLife"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:PerCent"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.initialLossOfLife">
            <xs:annotation>
               <xs:documentation>Percentage of initial life expectancy that has been lost as of the last life expectancy baseline. Represents</xs:documentation>
               <xs:documentation>(initial life expectancy - current life expectancy) / initial life expectancy.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="inUseState"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:InUseStateKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.inUseState">
            <xs:annotation>
               <xs:documentation>Indication of whether asset is currently deployed (in use), ready to be put into use or not available for use.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="kind"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AssetKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.kind">
            <xs:annotation>
               <xs:documentation>Kind of asset. Used in description of asset components in asset instance templates.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="lifecycleState"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AssetLifecycleStateKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.lifecycleState">
            <xs:annotation>
               <xs:documentation>Current lifecycle state of asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="lotNumber"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.lotNumber">
            <xs:annotation>
               <xs:documentation>Lot number for this asset. Even for the same model and version number, many assets are manufactured in lots.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="name"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#IdentifiedObject.name">
            <xs:annotation>
               <xs:documentation>The name is any free human readable and possibly non unique text naming the object.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="purchasePrice"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:Money"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.purchasePrice">
            <xs:annotation>
               <xs:documentation>Purchase price of asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="retiredReason"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:RetiredReasonKind"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.retiredReason">
            <xs:annotation>
               <xs:documentation>Reason asset retired.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="serialNumber"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.serialNumber">
            <xs:annotation>
               <xs:documentation>Serial number of this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="type"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.type">
            <xs:annotation>
               <xs:documentation>Utility-specific classification of Asset and its subtypes, according to their corporate standards, practices, and existing IT systems (e.g., for management of assets, maintenance, work, outage, customers, etc.).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ActivityRecords"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:ActivityRecord"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.ActivityRecords">
            <xs:annotation>
               <xs:documentation>All activity records created for this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Analytic"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:Analytic"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.Analytic">
            <xs:annotation>
               <xs:documentation>Analytic performed on this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AnalyticScore"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:AnalyticScore"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.AnalyticScore">
            <xs:annotation>
               <xs:documentation>Analytic result related to this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AssetContainer"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AssetContainer"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.AssetContainer">
            <xs:annotation>
               <xs:documentation>Container of this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AssetDeployment"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AssetDeployment"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.AssetDeployment">
            <xs:annotation>
               <xs:documentation>This asset's deployment.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AssetInfo"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:AssetInfo"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.AssetInfo">
            <xs:annotation>
               <xs:documentation>Data applicable to this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Measurements"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:Measurement"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.Measurements">
            <xs:annotation>
               <xs:documentation>Measurement related to this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Procedures"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:Procedure"
                     sawsdl:modelReference="http://iec.ch/TC57/CIM100#Asset.Procedures">
            <xs:annotation>
               <xs:documentation>All procedures applicable to this asset.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AssetContainer"
                   sawsdl:modelReference="http://iec.ch/TC57/CIM100#AssetContainer">
      <xs:annotation>
         <xs:documentation>Asset that is aggregation of other assets such as conductors, transformers, switchgear, land, fences, buildings, equipment, vehicles, etc.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:Asset">
            <xs:sequence>
               <xs:element name="mRID"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:string"
